/ fim for\n  }\n\n  message += `<table>\n<tbody>\n<tr>\n<td><img style=\"vertical-align:middle\" src=${chatImagem} width=\"32\" height=\"32\"></td>\n<td><h2 style=\"color:Green\"><b>Terra Devastada</b></h2></td>\n</tr>\n</tbody>\n</table>`;  \n  message+=`<p>Você rolou ${dice} dado de sorte, e alcançou <b style=\"color:Green\">${explosoes}</b> explosões.</p>`;\n  \n   if (sorte) {\n    message+=`<h3>Sorte</h3>`;\n    message+=`<p>Sua <b style=\"color:Green\">sorte</b> lhe agraciou com <b style=\"color:Green\">${sucessossorte}</b> sucesso(s).</p>`;\n    message+=`<p style=\"background-color: lightgray; text-align: center;\"><b>${rolagenssorte.join()}</b></p>`;\n  }\n\n  if (secreto) {\n    chatData = {\n      content: message,\n      whisper : ChatMessage.getWhisperRecipients(\"GM\")\n    };     \n  } else {\n    chatData = {\n      content: message      \n    };     \n  } \n  \n  if (som) {\n    AudioHelper.play({src: som, volume: volumeDoSom, autoplay: true, loop: false}, true);\n  }\n  \n  ChatMessage.create(chatData, {});  \n  /*\n  for (var i = 0; i<rolagens3D.length; i++) {    \n    let tempChatData = {\n      roll: rolagens3D[i],\n      rollMode: game.settings.get(\"core\", \"rollMode\"),\n      content: `<p>Normal: ${rolagens3D[i].total}</p>`\n    };     \n    ChatMessage.create(tempChatData);  \n  }\n  for (var i = 0; i<rolagens3Dsorte.length; i++) {    \n    let tempChatData = {\n      roll: rolagens3Dsorte[i],\n      rollMode: game.settings.get(\"core\", \"rollMode\"),      \n      content: `<p>Sorte: ${rolagens3D[i].total}</p>`\n    };     \n    ChatMessage.create(tempChatData);  \n  }  \n*/\n})(); // fim async","folder":null,"sort":0,"flags":{"combat-utility-belt":{"macroTrigger":""},"core":{"sourceId":"Macro.TDY11kKgfTl7Ayp5"}},"_id":"d0exHogW4jrBtGGw","ownership":{"default":0,"2D75wq8Se8IzqvA9":3,"s8nlS6EfgrBPcWgH":3},"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}
