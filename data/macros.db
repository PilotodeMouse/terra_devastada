lagens3D.push(roll3d);\n\n      if(roll==6){\n        sucessossorte+=1;\n        if(roll==6){\n          sortedice+=1;          \n        }\n      }    \n    } // fim for\n  }\n\n  message += `<table>\n<tbody>\n<tr>\n<td><img style=\"vertical-align:middle\" src=${chatImagem} width=\"32\" height=\"32\"></td>\n<td><h2 style=\"color:Crimson\"><b>Terra Devastada</b></h2></td>\n</tr>\n</tbody>\n</table>`;  \n  message+=`<p>Você rolou ${dice} dado(s), e alcançou <b style=\"color:Navy\">${explosoes}</b> explosões.</p>`;\n  \n  message+=`<h3>Consequência:</h3>`;\n  message+=`<p style=\"background-color: lightgrey;\">Você obteve <b style=\"color:Crimson\">${sucessos}</b> sucesso(s).</p>`;  \n  \n  if (sorte) {\n    message+=`<h3>Sorte</h3>`;\n    message+=`<p>Sua <b style=\"color:Green\">sorte</b> foi <b style=\"color:Red\">${sucessossorte}</b> sucesso(s).</p>`;\n    message+=`<p style=\"background-color: lightgray; text-align: center;\"><b>${rolagenssorte.join()}</b></p>`;\n    message+=`<h3>Total</h3>`;\n    message+=`<p style=\"background-color: lightgray;\">Seu total de sucessos é de <b style=\"color:Red\">${sucessos+sucessossorte}</b> sucesso(s).</p>`;\n  }\n\n  if (secreto) {\n    chatData = {\n      content: message,\n      whisper : ChatMessage.getWhisperRecipients(\"GM\")\n    };     \n  } else {\n    chatData = {\n      content: message      \n    };     \n  } \n  \n  if (som) {\n    AudioHelper.play({src: som, volume: 1.0, autoplay: true, loop: false}, true);\n  }\n  \n  ChatMessage.create(chatData, {});  \n  \n  for (var i = 0; i<rolagens3D.length; i++) {    \n    let tempChatData = {\n      type: CHAT_MESSAGE_TYPES.ROLL,\n      roll: rolagens3D[i],\n      rollMode: game.settings.get(\"core\", \"rollMode\"),\n      content: `<p>Resultado: ${rolagens3D[i].total}</p>`\n    };     \n    ChatMessage.create(tempChatData);  \n  }\n\n  for (var i = 0; i<rolagens3Dsorte.length; i++) {    \n    let tempChatData = {\n      type: CHAT_MESSAGE_TYPES.ROLL,\n      roll: rolagens3Dsorte[i],\n      rollMode: game.settings.get(\"core\", \"rollMode\"),      \n      content: `<p>Sorte: ${rolagens3D[i].total}</p>`\n    };     \n    ChatMessage.create(tempChatData);  \n  }  \n\n})(); // fim async","author":"qNlha4RcPR9L4hhv","img":"worlds/protocolo_zero/Icones/dado3.jpg","_id":"PGUru9JNHpcNisjx","ownership":{"default":0,"2D75wq8Se8IzqvA9":3,"qNlha4RcPR9L4hhv":3},"folder":null,"_stats":{"systemId":null,"systemVersion":null,"coreVersion":null,"createdTime":null,"modifiedTime":null,"lastModifiedBy":null}}
